@using Proje01.Models.Siniflar
@model List<Blog>


<div class="padding">
    <h4>Haftanın Enleri</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
        <li class="list-bg"><span>@{i++;}@i.</span><a href="/Oneri/BlogDetay/@x.ID">@x.Baslik</a></li> 
        }
    </ul>
    <a href="/Oneri/Index" class="link1">Daha fazlasını gör</a>
</div>