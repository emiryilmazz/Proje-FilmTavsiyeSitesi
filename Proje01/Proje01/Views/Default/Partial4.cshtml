@using Proje01.Models.Siniflar
@model List<Blog>


<div class="padding1">
    <h4>Editörün Seçimleri</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
        <div class="bot-img">
            <img src="~/icerik/film_res/@x.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.Aciklama.Substring(0,75)...</p>
        </div>
        <div class="clear"></div>
        }  
    </div>
    @Html.Action("Partial5","Default")
</div>