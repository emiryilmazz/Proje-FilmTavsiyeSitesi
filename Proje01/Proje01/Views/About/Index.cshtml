@using Proje01.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_View_Layout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda Pek fazla bir şey yok :D</h3>
            
            @foreach (var x in Model)
            {

                <img src="@x.FotoUrl" alt="" width="900" height="600"/>
                <p>@x.Aciklama</p>
            }
        </div>
    </div>
</div>

